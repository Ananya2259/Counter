import java.util.*;
 public class Counter {
  public static void main(String args[]){
    Scanner kbd = new Scanner(System.in);
    System.out.print("Enter a four digit number:");
    int digit = kbd.nextInt();
    int r = digit % 1000;
    int q = digit / 1000;
    System.out.println("Counter clock wise:"+r+""+q);
  }
}
